{
    "name": "odm-one-download-manager",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run setup:ytdlp && next build",
        "start": "node server.js",
        "lint": "next lint",
        "setup:ytdlp": "node scripts/setup-ytdlp.js",
        "prebuild": "npm run setup:ytdlp",
